						}
						opt += '</select>';
						document.getElementById('q_list').innerHTML = opt;
					}
					
					function add_search_goods(obj)
					{
						var order_id = document.forms['theForm'].elements['order_id'].value;
						var good_id = document.forms['theForm'].elements['good_id'].value;
						var quantity = document.forms['theForm'].elements['quantity'].value;
						var flag = document.forms['theForm'].elements['flag'].value;
						var children = document.getElementById('goods_list').childNodes;
						var peer_flag = '';
						if (children.length > 0){
							peer_flag = children[0].firstChild.attributes[1].nodeValue.split(',')[3];
							
						}
						var goods_name = '';
						var len = document.forms['theForm'].elements['good_id'].options.length;
						for (var i = 0; i < len; ++i)
						{
							if (document.forms['theForm'].elements['good_id'].options[i].selected)
							{
								goods_name = document.forms['theForm'].elements['good_id'].options[i].innerHTML;
								break;
							}
						}
						if (order_id == '0' || good_id == '0' || quantity == '0' || flag == '0' || document.getElementById(order_id + '_' + good_id))
						{
							return ;
						}
						if (flag != peer_flag && peer_flag != ''){
							alert('每次只能选一种服务');
							return;
						}
						var goods_div = document.createElement("div");
						goods_div.id = order_id  + '_' + good_id;
						goods_div.innerHTML = '<input type="hidden" name="multi_goods_id[]" value="' + order_id + ',' + good_id + ',' + quantity + ',' + flag + '">' + goods_name + '&nbsp;&nbsp;<img style="cursor: pointer;" onclick="del_search_goods(\'' + order_id  + '_' + good_id + '\');" src="images/no.gif"/>';
