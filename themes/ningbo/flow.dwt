        <script>
            function updateCart(){
                var f=document.forms["formCart"];
                f.step.value="update_cart";
            }
            function checkout(){//跨境城购物车改造


                var f=document.forms["formCart"];
                /*购物车改造2 398行开始共计8行*/
                if (f.kj_agree){
                    if (f.kj_agree.checked == false){
                        alert("请您同意协议后再进行购物，谢谢合作！");
                        return;
                    }
                }
                /*购物车改造2 结束*/
                f.step.value="checkout";
                f.update_selected.value="yes";
                $("#formCart").find("input[name='submit']").click();
                f.step.value="update_cart";
                f.update_selected.value="no";
            }
            //购物车改造 全选框操作，选择框勾选金额的变动
            $(function(){
                var currency_format="{$currencyFormat}";
                var shopping_money_lang="{$shopping_money_lang}";
                $("#checkAll").click(function(){
                    if($("#checkAll").prop("checked")){
                        $("input[name^=goodsSelect]").prop("checked",true);
                    }else{
                        $("input[name^=goodsSelect]").prop("checked",false);
                    }
                    computeShoppingMoney();
                });
                $("input[name^=goodsSelect]").bind("click",function(){
                    computeShoppingMoney();
                });
                //计算选择商品的购物金额
                function computeShoppingMoney(){
                    var total=0.00;
                    $("span[name=subtotal]").each(function(idx,e){
                        var subtotal=$(e).html().split(currency_format)[1];
                        if( $("input[name^=goodsSelect]").eq(idx).prop("checked"))
                            total+=parseFloat(subtotal);
                    });
                    $("#shopping_money").html(shopping_money_lang+fmoney(total,2));
                }
                function fmoney(s, n){//格式化金额
                    n = n > 0 && n <= 20 ? n : 2;
                    s = parseFloat((s + "").replace(/[^\d\.-]/g, "")).toFixed(n) + "";
                    var l = s.split(".")[0].split("").reverse(),
                            r = s.split(".")[1];
                    t = "";
                    for(i = 0; i < l.length; i ++ ){
                        //t += l[i] + ((i + 1) % 3 == 0 && (i + 1) != l.length ? "," : ""); 三位分割
                        t += l[i] + ((i + 1) % 3 == 0 && (i + 1) != l.length ? "" : "");
                    }
                    return currency_format+t.split("").reverse().join("") + "." + r;
                }

            });
        </script>

